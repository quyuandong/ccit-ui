{
  "name": "ccit-ui-design",
  "private": false,
  "version": "0.1.2",
  "type": "module",
  "description": "Vue3中基于ant-design-vue二次封装的组件库",
  "author": {
    "name": "quYuanDong",
    "email": "1984602738@qq.coom"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://quyuandong.github.io/ccit-ui/"
  },
  "homepage": "https://quyuandong.github.io/ccit-ui/",
  "bugs": "https://github.com/quyuandong/ccit-ui/issues",
  "main": "lib/ccit-ui.umd.js",
  "module": "lib/ccit-ui.umd.js",
  "types": "lib/index.d.ts",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "lib",
    "global.d.ts"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "docs:dev": "vitepress dev docs --host",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "docs:deploy": "sh deploy.sh",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx",
    "lint:fix": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "ac": "git add . && cz",
    "cm": "cz",
    "release": "release-it",
    "test:push": "yarn build && yalc push"
  },
  "dependencies": {
    "@formily/vue": "^2.3.0",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vitepress-demo-preview/component": "^2.3.2",
    "@vitepress-demo-preview/plugin": "^1.2.2",
    "@vue/composition-api": "^1.7.2",
    "element-ui": "^2.15.14",
    "eslint": "^8.50.0",
    "vite-plugin-compression": "^0.5.1",
    "vue": "2.7.15",
    "vue-template-compiler": "^2.7.15"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@release-it/conventional-changelog": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "commitizen": "^4.3.0",
    "cz-git": "^1.7.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.30",
    "postcss-preset-env": "^9.1.4",
    "prettier": "^3.0.3",
    "release-it": "^16.2.1",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "terser": "^5.21.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.0",
    "vitepress": "^1.0.0-rc.12",
    "vue-tsc": "^1.8.5"
  },
  "lint-staged": {
    "config": "./.lintstagedrc.cjs"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "keywords": [
    "vite",
    "vite3",
    "vitepress",
    "ccit",
    "vue",
    "vue3",
    "ccit-ui",
    "二次封装组件",
    "封装组件",
    "components"
  ]
}
